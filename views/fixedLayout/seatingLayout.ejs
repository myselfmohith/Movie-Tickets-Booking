<style>
  .bookcontainer {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    min-width: 90vw;
    margin: 25px 0;
  }

  .struct {
    font-family: sans-serif;
    font-size: 16px;
    width: 20px;
    height: 20px;
    display: flex;
    justify-content: center;
    align-items: center;
    color: grey;
    transition: 0.3s;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
  }

  .seat {
    font-size: 13px;
    border: solid 0.01px grey;
    border-radius: 3px;
    cursor: pointer;
  }

  .seat:hover {
    z-index: 999;
    transform: scale(1.2);
  }

  .seatbooked {
    background-color: rgb(13, 20, 24, 25);
    opacity: 0.2;
  }

  .seatbooked:hover {
    cursor: auto;
    transform: none;
  }

  .seatselected {
    border-radius: 6px;
    transform: scale(1.09);
    background-color: lightgreen;
    border-color: lightgreen;
  }

  .table-responsive {
    max-width: 90vw;
    overflow-x: auto;
  }

  .table-responsive h1 {
    text-align: center;
    background-color: rgb(41, 41, 33, 0.2);
    color: grey;
    position: sticky;
    left: 0;
    border-radius: 40px 40px 0 0;
    font-size: 25px;
  }

  .ticketDiv {
    max-width: 100%;
    margin: 25px;
    display: flex;
    padding: 10px;
    overflow: auto;
  }

  .ticketDiv .movieimage {
    width: 100px;
    height: 150px;
    background-size: cover;
    background-position: center;
  }

  .ticketDiv table {
    margin-left: 25px;
  }

  .ticketDiv th,
  .ticketDiv td {
    width: 125px;
    text-align: left;
    vertical-align: top;
    padding: 5px;
  }

  .ticketDiv td {
    width: 200px;
    padding: 5px 10px;
  }

  .ticketDiv a {
    display: inline-block;
    margin: 20px auto;
    font-size: 20px;
    padding: 5px 25px;
    cursor: pointer;
    border: solid white 0.8px;
    border-radius: 10px;
    background-color: transparent;
    color: white;
  }

  .ticketDiv a:hover {
    background-color: white;
    color: rgb(13, 20, 24);
  }

  .legend{
    text-align: center;
  }

  .legend .sdum {
    font-size: 13px;
    border: solid 0.01px grey;
    border-radius: 3px;
    cursor: pointer;
  }

  .legend * {
    display: inline-block;
    margin-left: 5px;
    vertical-align: baseline;
  }

  @media print {
    * {
      visibility: hidden;
    }

    body {
      background-color: transparent;
    }

    .ticketDiv * {
      visibility: visible;
    }

    .ticketDiv a {
      visibility: hidden;
    }

    .ticketDiv {
      position: absolute;
      top: 0;
      left: 0;
    }
  }
</style>

<!-- Scripts -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script>
  // No error Runtime
  const jsonfile = JSON.parse('<%-jsonlayout %>');
  const blockedseats = "<%=movie.totalDays[cdate].shows[show].bookedSeats %>".split(",");
  if (blockedseats.length != 1) blockedseats.forEach(ele => $("#" + ele).addClass("seatbooked"));


  if (window.location.href[window.location.href.length - 1] != "/") { window.location.href += "/"; }
  let selectedseats = [];
  let price = 0;
  let linkgen = ".";
  linkgen = window.location.href
  $("#printtickets").attr("href", linkgen);
  $(".seat").click(function () {
    if (!($(this).hasClass("seatbooked"))) {
      let id = $(this).attr("id");
      if (selectedseats.includes(id)) {
        selectedseats = selectedseats.filter(item => item !== id);
        price = price - Number(jsonfile[id] || jsonfile["default"]);
      }
      else {
        if (selectedseats.length > 5) {
          alert('You can Slect Only 6 Seats at Max');
          return;
        }
        selectedseats.push(id);
        price = price + Number(jsonfile[id] || jsonfile["default"]);
      }
      $("#checkedseats").text(selectedseats.toLocaleString());
      linkgen = window.location.href + selectedseats.toLocaleString();
      $("#printtickets").attr("href", linkgen);
      $("#totalcost").text(price + "rs");
      $(this).toggleClass("seatselected");
    }
  });

</script>