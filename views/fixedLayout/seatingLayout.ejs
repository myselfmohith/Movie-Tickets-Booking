<!--  30 colums bottom to top i.e, V to A -->
<div class="bookcontainer">
  <div>
    <div class="ticketDiv">
      <div class="movieimage" style="background-image: url('<%= movie.imageURL %>')"></div>
      <div class="ticketinfo">
        <table>
          <tr>
            <th>Movie Name</th>
            <td>
              <%=movie.movieName%>
            </td>
          </tr>
          <tr>
            <th>Date</th>
            <td>
              <%= movie.totalDays[cdate].date %>
            </td>
          </tr>
          <tr>
            <th>Show Time</th>
            <td>
              <%= movie.totalDays[cdate].shows[show].showTime %>
            </td>
          </tr>
          <tr>
            <th>Seats</th>
            <td id="checkedseats"></td>
          </tr>
          <tr>
            <th>Ticket Price</th>
            <td id="totalcost">0rs</td>
          </tr>
        </table>
        <a href="" id="printtickets" onclick="javascript:window.print()">Print Tickets</a>
      </div>
    </div>
    <div class="legend">
      <div class="struct sdum">n</div><span>Empty</span>
      <div class="struct sdum seatselected">n</div><span>Selected</span>
      <div class="struct sdum seatbooked">n</div><span>Booked</span>
    </div>
  </div>

